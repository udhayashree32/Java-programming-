public class Main {
    public static void main(String[] args) {

        int[][] arr = {
            {3, 4, 5},
            {6, 7, 8},
            {9, 10, 11}
        };


        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                System.out.print(arr[i][j] + " ");
            }
            System.out.println();
        }
    }
}

OUTPUT:
3 4 5 
6 7 8 
9 10 11 

